  <script type="text/javascript" src="../js/easydialog.js"></script>
  <script type="text/javascript">
      $(window).load(function(){
          $('#demod').click(function(){
              easyDialog.open({
                  container : {
                      header : '点我弹出层',
                      content : '欢迎访问通用样式页面 ！ ',
                      yesFn : btnFn,
                      noFn : true
                  },
                  fixed : true
              });
          });
          $('#demoBtn1').click(function(){
              easyDialog.open({
                  container : 'probx',
                  fixed : true
              });
          });
          $('#demoBtn2').click(function(){
              easyDialog.open({
                  container : 'proBox',
                  fixed : true
              });
          });

          /*关闭弹出层*/
          $(".close_btn,.easyDialogNoBtn").click(function(){
              easyDialog.close();
          })

          /*确认按钮回掉函数*/
          function btnFn(){
              alert("zzt.tm");
              //return false;
          }
      })
  </script>
