@charset "utf-8";
@import "zzt.scss";
.select-area {
  //width: 97%;
  margin:0 auto 1%;
  background-color: $white;
  @include borderRadius(4px);
  padding: 2% 1%;
  span{
    display: inline-block;
    margin-right: 2%;
    width: 15%;
    white-space: nowrap;
    margin-bottom: 1%;
  }
  .last-line {
    margin-bottom: 0;
  }
  label {
    display: inline-block;
    vertical-align: middle;
    height: 34px;
    line-height: 34px;
  }

}
input, select {
  display: inline-block;
  vertical-align: middle;
  width: 67%;
  height: 28px;
  line-height: 28px;
  padding: 2px;
  @include borderRadius(4px);
  border: 1px solid $bordercolor;
}
select {
  width: 70%;
  padding: 8px;
  height: auto;
}
.select-btn{
  margin-top: 1%;
  overflow: hidden;
  width: 32%;
  .btn{
    float: right;
    margin-left: 3%;
  }
}
.pro-img{
  border: 1px solid #ddd;
}
.table-check{
  width: auto;
  height: auto;
  padding: 0;
  border: 0 none;
}
.select-tab{
  width: 150px;
  //margin-right: 1%;
}
.tab1-table{
  margin-top: 1%;
}
.page-container{
  width: 225px;
  margin: 0 auto;
}
.review-title{
  color: #666;
  font-size: 18px;
  overflow: hidden;
  a{
    font-size: 12px;
    margin-left: 0.7%;
    &.look{
      margin-left: 6%;
    }
  }
  span{
    font-weight: normal;
    display: inline-block;
    vertical-align: middle;
    width: 70%;
    //margin-left: 2%;
  }
}
.reason{
  width: 45%;
}
.sub-title{
  font-size: 12px;
  color: #666;
  margin-bottom: 1%;
  margin-top: 0.5%;
}
.channel-title{
  overflow: hidden;
  b{
    color: #666;
    font-size: 18px;
    line-height: 36px;
    float: left;
  }
  span{
    float: right;
  }
  a{
    float: left;
    height: 36px;
    line-height: 36px;
    margin-left: 1.2%;
  }
}
.hint{
  font-size: 12px;
  color: #333;
  //margin-top: 0.5%;
  vertical-align: middle;
}
.star{
  color: red;
  font-style: normal;
  vertical-align: middle;
}
table .channel-name{
  text-align: left;
  padding-left: 1.5%;
}
.channel2{
  display: none;
}
.enter-wrapper{
  padding-bottom: 1.5%;
  border-bottom: 1px solid $bordercolor;
}
.select-label{
  display: inline-block;
  vertical-align: middle;
}
.related-pro{
  position: relative;
  margin: 1% 0 0;
  .related-input{
    width: 360px;
  }
}
.reduce{
  width: 189px;
  margin: 1% 0;
  input{
    width: 40%;
  }
}
.pro-msg{
  margin-top: 1%;
  width: 720px;
  margin-left: 4%;
}
.desc-label{
  display: inline-block;
  vertical-align: top;
}
#desc{
  @include borderRadius(4px);
  border: 1px solid $bordercolor;
  padding: 2px;
  width: 67%;
  height: 130px;
  resize: none;
}
.disabled{
  color: #ddd;
}
.format{
  span{
    float: left;
    width: 100px;
    height: 50px;
    line-height: 50px;
    text-align: center;
    border: 1px solid $bordercolor;
    margin-left: -1px;
    margin-bottom: -1px;
  }
  .add{
    margin-top: 10px;
    float: left;
    width: 199px;
    margin-left: 0.7%;
  }
  .add-input{
    width: 60%;
    margin-left: 1%;
  }
}
.format-table{
  width: 30%;
  border: 1px solid $bordercolor;
}
.pic-list{
  overflow: hidden;
  li{
    width: 100px;
    height: 100px;
    border: 1px solid $bordercolor;
    float: left;
    margin: 0 10px 10px 0;
    &.pic-add {
      a{
        color: #78cd51;
        font-size: 60px;
        font-weight: bold;
        display: block;
        width: 100%;
        height: 100%;
        text-align: center;
        background: url("../img/add.png") no-repeat center center;
        &:hover{
          text-decoration: none;
        }
      }
    }
  }
}
.cancel{
  width: 25px;
  height: 25px;
  line-height: 22px;
  top: 0;
  right: 0;
  background: red;
  img{
    display: block;
    margin: 5.5px auto 0;
  }
}
.disabledbg{
  background-color: #ddd;
}
.sort{
  width: 12px;
  height: 8px;
  top: 46px;
}
.sort-l{
  background: url("../img/sort-arrow.png") no-repeat;
  left: 2px;
}
.sort-r{
  background: url("../img/sort-arrowr.png") no-repeat;
  right: 2px;
}
.property{
  .spec{
    input{
      width: 19.2%;
      margin-right: 1%;
    }
  }
  label{
    width: 80px;
    display: inline-block;
    text-align: right;
  }
}
.checkgroup{
  display: inline-block;
  padding: 2% 2% 0;
  background-color: $tablebg;
  border: 1px solid $bordercolor;
  vertical-align: top;
  width: 15.7%;
  margin-right: 1%;
  em{
    margin-bottom: 15px;
    display: block;
    input{
      margin-right: 10px;
    }
  }
}
.custom{
  display: inline-block;
  vertical-align: middle;
  margin-right: 1%;
  margin-bottom: 1%;
  input{
    width: 120px;
  }
  &.edit{
    background-color: $tablebg;
    padding: 5px;
    @include borderRadius(4px);
  }
}
.x{
  display: inline-block;
  vertical-align: middle;
  color: #333;



  &:hover{
    text-decoration: none;
  }
}
.size-fillin{
  width: 50%;
}
.file{
  display: block;
  width: 100%;
  height: 100%;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  cursor: pointer;
  filter: alpha(opacity=0);
}
.prompt{
  .f12{
    display: block;
    text-align: left;
    margin: 2% 0 3%;
  }
}
#aa{
  width: 487px;
}
.easyDialog_wrapper{
  background-color: $white;
}
.look2-content{
  padding: 5px 10px;
}
.encoded-edit{
  display: none;
  input{
    width: 10%;
    margin-right: 1%;
  }
}
.mark{
  width: 15px;
  height: 15px;
  line-height: 14px;
  font-size: 12px;
  border-radius: 100%;
  background-color: $red;
  color: $white;
  position: absolute;
  top: -7.5px;
  right: 5%;
  text-align: center;
  font-style: normal;
  z-index: 10;
}
.panel{
  overflow: visible;
}
.pro-type{
  white-space: nowrap;
}
.encoded-btn,.look{
  display: inline-block;
  line-height: 20px;
}
#format-add,#custom-edit{
  display: none;
}
.pro-list{
  width: 364px;
  position: absolute;
  display: none;
  border: 1px solid $bordercolor;
  border-top: 0 none;
  top: 34px;
  background-color: #fff;
  li{
    a{
      display: block;
      color: #333;
      height: 30px;
      line-height: 30px;
      padding: 5px 10px;
      &:hover{
        background-color: $tablebg;

      }
    }
  }
}